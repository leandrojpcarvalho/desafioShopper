{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "**/*.test.ts",
        "node_modules",
        "src/interface",
        "src/app.ts",
        "src/server.ts",
        "src/utils/types.ts",
        "src/utils/EnumError.ts"
    ],
    "reporter": [
        "text",
        "html",
        "lcov"
    ],
    "check-coverage": true,
    "statements": 90,
    "branches": 90,
    "functions": 90,
    "lines": 90
}